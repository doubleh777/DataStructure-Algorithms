package findmaxsubarray;

public class FindMaxSubarrayTest {
	
	public static void main(String[] args){
		
		FindMaxSubarray_Square findMaxSubarray = new FindMaxSubarray_Square();
		
		int[] arr0 = null;
		int[] arr1 = {};
		int[] arr2 = {4};
		int[] arr3 = {1, 9};
		int[] arr4 = {-1, 4, 9, -1};
		int[] arr5 = {2,-4,-2, 1, 5, 10,-4 , 4, 6, -1, -4, 10, -1, 2, -10, 3, -2, 1, 7, -2};
		SubarrayInfo maxSubarray = new SubarrayInfo();
		
		maxSubarray = findMaxSubarray.find_Square(arr0);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
		maxSubarray = findMaxSubarray.find_Square(arr1);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
		maxSubarray = findMaxSubarray.find_Square(arr2);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
		maxSubarray = findMaxSubarray.find_Square(arr3);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
		maxSubarray = findMaxSubarray.find_Square(arr4);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
		maxSubarray = findMaxSubarray.find_Square(arr5);
		System.out.println("Front : " + maxSubarray.getFront() + ", " + "End : " + maxSubarray.getEnd() + ", " + "Sum : " + maxSubarray.getSum());
		
	
	}
}
